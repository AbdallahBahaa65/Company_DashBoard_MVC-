@model string

<td>
    <a class="btn " asp-action="Details" asp-route-id="@Model">@* @Model Represent the ID  *@
        <i class="fas fa-eye text-primary"></i>
    </a>
</td>
<td>
    <a class="btn " asp-action="Update" asp-route-id="@Model">@* @Model Represent the ID  *@
        <i class="fas fa-edit  text-warning"></i> 
    </a>
</td>

<td>
    <a class="btn " asp-action="Delete" asp-route-id="@Model">@* @Model Represent the ID  *@
        <i class="fa-solid fa-trash   text-danger"></i> 
    </a>
</td>